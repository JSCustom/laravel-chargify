{
  "name": "jscustom/laravel-chargify",
  "description": "Laravel package for Chargify integration.",
  "type": "library",
  "require": {
    "php": "^7.0|^8.0",
    "illuminate/support": "^7.0|^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0 || ^6.0 || ^7.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "JSCustom\\Chargify\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Jediael Saliba",
      "email": "developer.jeddsaliba@gmail.com"
    }
  ],
  "minimum-stability": "beta",
  "extra": {
    "laravel": {
      "providers": [
        "JSCustom\\Chargify\\Providers\\ChargifyServiceProvider"
      ],
      "aliases": {
        "Qbo": "JSCustom\\Chargify\\Facade"
      }
    }
  }
}
